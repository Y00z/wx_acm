
<!--pages/runaccountinfo/runaccountinfo.wxml-->
<view style="background-color: white">
<view class="runaccountinfo-root">
  <text class="runaccountinfo-title" wx:if="{{data.dataType == 'ao'}}" >第一台压缩机</text>
  
  <text class="runaccountinfo-title" wx:else> 第二台压缩机 </text>
      <view style="width:100%;height:2rpx;background-color:rgba(239, 239, 244, .8);margin:20rpx"></view>
  <text class="runaccountinfo-text">开机时间：{{data.openTime}}</text>
      <view style="width:100%;height:2rpx;background-color:rgba(239, 239, 244, .8);margin:20rpx"></view>
  <text class="runaccountinfo-text">关机时间：{{data.closeTime}}</text>
      <view style="width:100%;height:2rpx;background-color:rgba(239, 239, 244, .8);margin:20rpx"></view>
  <text class="runaccountinfo-text">运行时间：{{data.workTime}}</text>
      <view style="width:100%;height:2rpx;background-color:rgba(239, 239, 244, .8);margin:20rpx"></view>
  <text class="runaccountinfo-text">耗电量：{{data.powerCons}}(kw·h)</text>
      <view style="width:100%;height:2rpx;background-color:rgba(239, 239, 244, .8);margin:20rpx"></view>
  <text class="runaccountinfo-text">25%负荷：{{data.fuhe25Time}}</text>
      <view style="width:100%;height:2rpx;background-color:rgba(239, 239, 244, .8);margin:20rpx"></view>
  <text class="runaccountinfo-text">50%负荷：{{data.fuhe50Time}}</text>
      <view style="width:100%;height:2rpx;background-color:rgba(239, 239, 244, .8);margin:20rpx"></view>
  <text class="runaccountinfo-text">75负荷：{{data.fuhe75Time}}</text>
      <view style="width:100%;height:2rpx;background-color:rgba(239, 239, 244, .8);margin:20rpx"></view>
  <text class="runaccountinfo-text">100%负荷：{{data.fuhe100Time}}</text>
</view>
</view>